{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactoryM365toBlobStorage"
		},
		"AzureBlobStorage1_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'AzureBlobStorage1'"
		},
		"Office3651_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'Office3651'"
		},
		"AzureBlobStorage1_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://msgraphdcm365datastore.blob.core.windows.net"
		},
		"AzureBlobStorage1_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "f5f569b0-3bc5-4ce7-9b46-028fa3bb003b"
		},
		"AzureBlobStorage1_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "0fae668a-7e67-4a19-9d79-d1270ff9e1a2"
		},
		"Office3651_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "0fae668a-7e67-4a19-9d79-d1270ff9e1a2"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactoryM365toBlobStorage_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Office3651_servicePrincipalKey": {
						"value": "[parameters('Office3651_servicePrincipalKey')]"
					},
					"AzureBlobStorage1_servicePrincipalKey": {
						"value": "[parameters('AzureBlobStorage1_servicePrincipalKey')]"
					},
					"Office3651_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('Office3651_properties_typeProperties_servicePrincipalId')]"
					},
					"AzureBlobStorage1_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('AzureBlobStorage1_properties_typeProperties_serviceEndpoint')]"
					},
					"AzureBlobStorage1_properties_typeProperties_tenant": {
						"value": "[parameters('AzureBlobStorage1_properties_typeProperties_tenant')]"
					},
					"AzureBlobStorage1_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('AzureBlobStorage1_properties_typeProperties_servicePrincipalId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}